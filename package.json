{
    "name": "vscode-aql",
    "displayName": "AQL (ArangoDB Query Language) Syntax Highlighting",
    "description": "Syntax Highlighting for ArangoDB Query Language (AQL) ðŸ™Œ",
    "version": "1.7.1",
    "icon": "arangodb-logo.png",
    "publisher": "monotykamary",
    "engines": {
        "vscode": "^1.40.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/monotykamary/vscode-aql"
    },
    "contributes": {
        "languages": [
            {
                "id": "aql",
                "aliases": [
                    "AQL",
                    "aql"
                ],
                "extensions": [
                    ".aql"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "aql",
                "scopeName": "source.aql",
                "path": "./syntaxes/aql.tmLanguage.json"
            },
            {
                "injectTo": [
                    "source.js",
                    "source.ts",
                    "source.jsx",
                    "source.tsx",
                    "source.go"
                ],
                "scopeName": "inline.aql",
                "path": "./syntaxes/inline-aql.json",
                "embeddedLanguages": {
                    "meta.embedded.block.aql": "aql"
                }
            }
        ]
    },
    "devDependencies": {
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-conventional": "^11.0.0",
        "husky": "^4.3.0",
        "standard-version": "^9.0.0"
    }
}
